{
  "name": "pghbeer-api",
  "version": "1.0.0",
  "description": "Nest Babel starter repository",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "start": "babel-node index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build": "babel -d lib/ src"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/microservices": "^6.10.1",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/websockets": "^6.10.1",
    "@nestjsplus/massive": "^1.2.0",
    "cache-manager": "^2.10.1",
    "core-js": "3.4.1",
    "dotenv": "^10.0.0",
    "massive": "^6.1.1",
    "moment": "^2.24.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@nestjs/testing": "^6.10.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "src",
    "testRegex": ".spec.js$",
    "coverageDirectory": "../coverage"
  }
}
